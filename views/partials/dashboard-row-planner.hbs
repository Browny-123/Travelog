<td class="table-division">
    <div>{{this.oifID}}</div>
</td>
<td class="table-division">
    <div>{{this.college.collegeName}}</div>
</td>
<td class="table-division">
    {{#each this.tripDates}}
    <div>{{format-date this 'DD/MM/YY' }}</div>
    {{/each}}
</td>
<td class="table-division">
    {{#each this.sessionInfo}}
    <div>{{this.name}} : {{this.timings}}</div>
    <br>
    {{/each}}
</td>
<td class="table-division">
    <div>{{this.numberOfBatches}}</div>
</td>

<td class="table-division">
    {{#unless this.trainers}}
    No trainers assigned yet.

    {{/unless}}
    {{#each this.trainers}}
    <div>{{this.name}}</div>
    {{/each}}
    <a href="" id="addTrainer~{{this._id}}" class="fa fa-user-plus table-icon addTrainerButton"></a>
</td>
<td class="table-division">
    {{#unless this.scheduleStatus}}
    <div>Unplanned</div>
    {{/unless}}
    {{#if this.scheduleStatus}}
    <div>Planned</div>
    {{/if}}
</td>
<td class="table-division">
    <a href="/edit-trip/{{this._id}}" class="fa fa-edit table-icon"></a>
</td>
<td class="table-division">
    <a href="" id="{{this._id}}" class="fa fa-trash table-icon tripDeleteButton"></a>
</td>
<td class="table-division">
    <a href="/planthetrip/{{this._id}}" class="fa fa-clipboard-list table-icon"></a>
</td>